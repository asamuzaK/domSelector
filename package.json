{
  "name": "@asamuzakjp/dom-selector",
  "description": "A CSS selector engine.",
  "author": "asamuzaK",
  "license": "MIT",
  "homepage": "https://github.com/asamuzaK/domSelector#readme",
  "bugs": {
    "url": "https://github.com/asamuzaK/domSelector/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asamuzaK/domSelector.git"
  },
  "files": [
    "deno.json",
    "dist",
    "src",
    "types"
  ],
  "type": "module",
  "exports": {
    "import": "./src/index.js",
    "require": "./dist/cjs/index.js"
  },
  "types": "types/index.d.ts",
  "dependencies": {
    "bidi-js": "^1.0.3",
    "css-tree": "^2.3.1",
    "is-potential-custom-element-name": "^1.0.1"
  },
  "devDependencies": {
    "@types/css-tree": "^2.3.4",
    "benchmark": "^2.1.4",
    "c8": "^8.0.1",
    "chai": "^4.3.10",
    "commander": "^11.1.0",
    "esbuild": "^0.19.8",
    "eslint": "^8.54.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-regexp": "^2.1.1",
    "eslint-plugin-unicorn": "^49.0.0",
    "jsdom": "^23.0.1",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "sinon": "^17.0.1",
    "typescript": "^5.3.2",
    "wpt-runner": "^5.0.0"
  },
  "scripts": {
    "bench": "node benchmark/bench.js",
    "build": "npm run tsc && npm run lint && npm test && npm run compat",
    "compat": "npm-run-all -s compat-*",
    "compat-cjs": "esbuild --format=cjs --platform=node --outdir=dist/cjs/ --minify --sourcemap src/**/*.js",
    "compat-deno": "node scripts/index denoconf -i",
    "lint": "eslint --fix .",
    "test": "c8 --reporter=text mocha --exit test/**/*.test.js",
    "test-wpt": "npm run update-wpt && node test/wpt/wpt-runner.js",
    "tsc": "npx tsc",
    "update": "npm-run-all -s update-*",
    "update-wpt": "git submodule update --init --recursive --remote"
  },
  "version": "1.1.9"
}
